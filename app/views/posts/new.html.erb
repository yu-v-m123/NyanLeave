<h1>post#new</h1>
<%= form_with model: @post, local: true do |f| %>
<%= f.label :start %>
<%= f.date_field :start %>
<%= f.label :end %>
<%= f.date_field :end %>
<%= f.label :place %>
<%= f.select :place, {お世話係さんのお家: "お世話係さんのお家", 飼い主さんのお家: "飼い主さんのお家", どちらでもよい: "どちらでもよい"}, {include_blank: "選択してください"} %>
<%= f.label :feature %>
<%= f.text_area :feature %>
<%= f.submit "投稿する" %>
<% end %>
<%= link_to "マイページに戻る", user_path(current_user), class: "back-mypage-link" %>